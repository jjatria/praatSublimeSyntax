<plist version="0.2">
<dict>
  <key>fileTypes</key>
  <array>
    <string>praat</string>
    <string>script</string>
    <string>psc</string>
    <string>praat_script</string>
    <string>praatscript</string>
    <string>praat-script</string>
    <string>praat-batch</string>
    <string>proc</string>
  </array>
  <key>name</key>
  <string>Praat</string>
  <!--code folding-->
  <key>foldingStartMarker</key>
  <string>^\s*(for|if|form|procedure|repeat|while)\s</string>
  <key>foldingStopMarker</key>
  <string>^\s*(endfor|elsif|else|endif|endform|endproc|endwhile|until)\s*</string>
  <!--patterns-->
  <key>patterns</key>
  <array>
    <!--comments-->
    <dict>
      <key>match</key>
      <string>(^|\s)#(.*)$</string>
      <key>name</key>
      <string>comment.line.number-sign.source.praat</string>
    </dict>
    <!--quotations: double and simple, and regex escapes exeptions within quotations-->
    <dict>
      <!--Problem 1: negative lookbehind for the "^\s*(comment|exit)\s" cases not available-->
      <key>begin</key>
      <string>"</string>
      <key>beginCaptures</key>
      <dict>
        <key>0</key>
        <dict>
          <key>name</key>
          <string>punctuation.definition.string.begin.source.praat</string>
        </dict>
      </dict>
      <key>end</key>
      <string>"</string>
      <key>endCaptures</key>
      <dict>
        <key>0</key>
        <dict>
          <key>name</key>
          <string>punctuation.definition.string.end.source.praat</string>
        </dict>
      </dict>
      <key>name</key>
      <string>string.quoted.double.source.praat</string>
    </dict>
    <dict>
      <key>begin</key>
      <string>(\:|\s|\()'</string><!--Problem 1: Temporary (inelegant) solution to the apostrophe problem-->
      <key>beginCaptures</key>
      <dict>
        <key>0</key>
        <dict>
          <key>name</key>
          <string>punctuation.definition.string.begin.source.praat</string>
        </dict>
      </dict>
      <key>end</key>
      <string>'</string>
      <key>endCaptures</key>
      <dict>
        <key>0</key>
        <dict>
          <key>name</key>
          <string>punctuation.definition.string.end.source.praat</string>
        </dict>
      </dict>
      <key>name</key>
      <string>string.quoted.single.source.praat</string>
    </dict>
    <!--numerals-->
    <dict>
      <key>match</key>
      <string>\b[0-9]+\.?[0-9]*\b</string>
      <key>name</key>
      <string>constant.numeric.source.praat</string>
    </dict>
    <!--methods-->
    <!--<dict>
      <key>match</key>
